var author = "Rafael Santiago";

print(author);

var start = 0;
var end = 10;

if(start == end){

	print(start);

} else if(start < end) {

	var cursor = start;

	while (cursor < end) {
		print(cursor);
		cursor = cursor + 1;
	}

	print(cursor);

} else {

	var cursor = end;

	while (cursor > start) {
		print(cursor);
		cursor = cursor - 1;
	}

	print(cursor);

}
