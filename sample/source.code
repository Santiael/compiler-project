const author = "Rafael Santiago";

fn range(start, end) {
  var cursor = end - start;

  if(start <= end) {
    while (cursor < end) {
      cursor = cursor + 1;
      print(cursor);
    }
  } else {
    while (cursor > end) {
      cursor = cursor - 1;
      print(cursor);
    }
  }

  return 0;
}
